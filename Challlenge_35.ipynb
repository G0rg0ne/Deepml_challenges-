   "source": [
    "import numpy as np\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "a074e488",
   "metadata": {},
   "outputs": [],
   "source": [
    "def make_diagonal(x):\n",
    "    n = len(x)\n",
    "    output = np.zeros((n, n))\n",
    "    for i in range(n):\n",
    "        output[i, i] = x[i]\n",
    "    return output\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "0022b3a6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[1. 0. 0.]\n",
      " [0. 2. 0.]\n",
      " [0. 0. 3.]]\n"
     ]
    }
   ],
   "source": [
    "x = np.array([1, 2, 3])\n",
    "output = make_diagonal(x)\n",
    "print(output)\n"
   ]
  }
