   "id": "50f3ab09",
   "metadata": {},
   "outputs": [],
   "source": [
    "def log_softmax(scores: list) -> np.ndarray:\n",
    "    softmax_scores = np.exp(scores) / np.sum(np.exp(scores))\n",
    "    result = np.log(softmax_scores)\n",
    "    return result.round(4)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
